	<content><![CDATA[
vector<int> factorize;
for(int i = 2; i * i <= n; i++){
    while(n % i == 0){
        factorize.pb(i);
        n /= i;
    }
}
if(n != 1)factorize.pb(n);
sort(all(factorize));
]]></content>
